<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/select"
        android:drawable="@drawable/ic_trans_sd_to_devise"
        android:state_selected="true"/>

    <item
        android:id="@+id/inselect"
        android:drawable="@drawable/ic_trans_device_to_sd"
        android:state_selected="false"/>

    <transition
        android:fromId="@+id/inselect"
        android:toId="@+id/select"
        android:drawable="@drawable/anim_trans_dev_to_sd"/>
</animated-selector>