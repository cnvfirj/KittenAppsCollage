<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:name="group"
                android:pivotX="12"
                android:pivotY="12"
                android:scaleX="0.9"
                android:scaleY="0.9">
                <path
                    android:name="path"
                    android:pathData="M 12 2.439 C 6.836 2.438 2.65 6.624 2.651 11.788 C 2.65 16.952 6.836 21.138 12 21.137 C 17.164 21.138 21.35 16.952 21.349 11.788 C 21.35 6.624 17.164 2.438 12 2.439 Z"
                    android:strokeColor="#FFFFFF"
                    android:strokeWidth="2.5"/>
                <group
                    android:name="group"
                    android:pivotX="12"
                    android:pivotY="12">
                    <path
                        android:name="path_1"
                        android:pathData="M 13 9 L 11 9 L 11 7 L 13 7 L 13 9 Z M 13 17 L 11 17 L 11 11 L 13 11 L 13 17 Z"
                        android:fillColor="#FFFFFF"
                        android:strokeWidth="1"/>
                </group>
            </group>

        </vector>
    </aapt:attr>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathStart"
                android:duration="300"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="125"
                    android:duration="175"
                    android:valueFrom="1"
                    android:valueTo="1.6"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="125"
                    android:duration="175"
                    android:valueFrom="1"
                    android:valueTo="1.6"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
