<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:name="group"
                android:pivotX="12"
                android:pivotY="12"
                android:translateX="6.5"
                android:translateY="6.5"
                android:scaleX="0.75"
                android:scaleY="0.67"
                android:rotation="45">
                <path
                    android:name="path_3"
                    android:pathData="M 5.765 13.588 L 5.765 10.53 L 10.531 10.494 L 10.527 10.54 L 13.548 10.499 L 13.529 10.471 L 18.235 10.53 L 18.235 13.588 L 13.529 13.588 L 13.508 13.569 L 10.584 13.584 L 10.588 13.647 Z"
                    android:fillColor="#FFFFFF"
                    android:strokeColor="#FFFFFF"
                    android:strokeWidth="0.1"/>
            </group>
            <group
                android:name="group_1"
                android:pivotX="12"
                android:pivotY="12"
                android:translateX="-1.4"
                android:translateY="-1.4"
                android:scaleX="0.8"
                android:scaleY="0.8">
                <path
                    android:name="path_1"
                    android:pathData="M 2.471 12.059 C 2.471 6.764 6.711 2.471 11.942 2.471 C 17.172 2.471 21.412 6.764 21.412 12.059 C 21.412 17.354 17.172 21.647 11.942 21.647 C 6.711 21.647 2.471 17.354 2.471 12.059 Z"
                    android:strokeColor="#FFFFFF"
                    android:strokeWidth="1.6"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="150"
                    android:duration="150"
                    android:valueFrom="45"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="150"
                    android:duration="150"
                    android:valueFrom="0.75"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="150"
                    android:duration="150"
                    android:valueFrom="0.67"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:duration="300"
                    android:valueFrom="6.5"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:duration="300"
                    android:valueFrom="6.5"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_1">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="300"
                    android:valueFrom="0.8"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="300"
                    android:valueFrom="0.8"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:duration="300"
                    android:valueFrom="-1.4"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:duration="300"
                    android:valueFrom="-1.4"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/linear_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="path_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="strokeWidth"
                android:duration="300"
                android:valueFrom="1.6"
                android:valueTo="3"
                android:valueType="floatType"
                android:interpolator="@android:anim/linear_interpolator"/>
        </aapt:attr>
    </target>
</animated-vector>
